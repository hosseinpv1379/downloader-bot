      "get": {
        "tags": [
          "Post"
        ],
        "security": [
          {
            "api_key": []
          }
        ],
        "description": "Download & Post Information",
        "parameters": [
          {
            "name": "shortcode",
            "in": "query",
            "description": "دانود پست اینستاگرام با کد پست",
            "required": false,
            "schema": {
              "type": "string",
              "example": "CilCOMcOgyB"
            }
          },
          {
            "name": "share",
            "in": "query",
            "description": "دانود پست اینستاگرام با کد اشتراک گذاری",
            "required": false,
            "schema": {
              "type": "string",
              "example": ""
            }
          },
          {
            "name": "id",
            "in": "query",
            "description": "دانلود پست اینستاگرام با آیدی پست",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "result": {
                      "type": "object",
                      "properties": {
                        "media": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "type": {
                                "type": "string"
                              },
                              "url": {
                                "type": "string",
                                "description": "The URL or identifier for the media content"
                              },
                              "cover": {
                                "type": "string",
                                "description": "The URL or identifier for the cover image"
                              }
                            }
                          },
                          "example": [
                            {
                              "type": "photo",
                              "url": "...",
                              "cover": "..."
                            },
                            {
                              "type": "video",
                              "url": "...",
                              "cover": "..."
                            }
                          ]
                        },
                        "caption": {
                          "type": "string",
                          "description": "Caption for the media",
                          "example": "..."
                        },
                        "owner": {
                          "type": "object",
                          "properties": {
                            "username": {
                              "type": "string",
                              "example": "..."
                            },
                            "profile_pic_url": {
                              "type": "string",
                              "description": "The URL of the profile picture",
                              "example": "..."
                            },
                            "is_private": {
                              "type": "boolean",
                              "description": "Indicates if the account is private",
                              "example": false
                            },
                            "is_verified": {
                              "type": "boolean",
                              "description": "Indicates if the account is verified",
                              "example": true
                            },
                            "id": {
                              "type": "string",
                              "description": "The unique identifier of the owner",
                              "example": "123456789"
                            },
                            "hd_profile_pic_url_info": {
                              "type": "object",
                              "properties": {
                                "url": {
                                  "type": "string",
                                  "description": "The URL of the high-definition profile picture",
                                  "example": "..."
                                }
                              }
                            },
                            "full_name": {
                              "type": "string",
                              "description": "Full name of the owner",
                              "example": "..."
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "The request contains invalid parameters."
          },
          "401": {
            "description": "The authentication token is not provided."
          },
          "403": {
            "description": "The request could not be completed due to insufficient credit."
          },
          "404": {
            "description": "The requested post has been restricted or removed."
          },
          "409": {
            "description": "The requested path does not exist."
          },
          "500": {
            "description": "The server returned an empty response. Please try again later."
          }
        }
      }
    },
